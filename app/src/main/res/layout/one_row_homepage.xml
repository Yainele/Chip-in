<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rowHomePage"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="#4E4B4B">

    <androidx.cardview.widget.CardView
        android:layout_width="508dp"
        android:layout_height="158dp"
        android:background="#5C4F4F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#C1BEBE">

            <TextView
                android:id="@+id/memberAmountHomePage"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_marginStart="372dp"
                android:layout_marginLeft="372dp"
                android:layout_marginTop="97dp"
                android:layout_marginEnd="36dp"
                android:layout_marginRight="36dp"
                android:layout_marginBottom="16dp"
                android:text="TextView"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/avatarHomePage"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="11dp"
                android:layout_marginEnd="22dp"
                android:layout_marginRight="22dp"
                android:layout_marginBottom="7dp"
                app:layout_constraintBottom_toTopOf="@+id/fullAmountHomePage"
                app:layout_constraintEnd_toStartOf="@+id/creditNameHomePage"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/empty" />

            <TextView
                android:id="@+id/credittimeHomePage"
                android:layout_width="0dp"
                android:layout_height="22dp"
                android:layout_marginStart="9dp"
                android:layout_marginLeft="9dp"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp"
                android:text="TextView"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/creditornameHomePage"
                app:layout_constraintStart_toEndOf="@+id/avatarHomePage"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="HardcodedText" />

            <TextView
                android:id="@+id/creditNameHomePage"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="4dp"
                android:text="TextView"
                android:textSize="21sp"
                app:layout_constraintBottom_toTopOf="@+id/creditornameHomePage"
                app:layout_constraintEnd_toStartOf="@+id/creditornameHomePage"
                app:layout_constraintStart_toEndOf="@+id/avatarHomePage"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/creditornameHomePage"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginEnd="118dp"
                android:layout_marginRight="118dp"
                android:layout_marginBottom="7dp"
                android:text="TextView"
                app:layout_constraintBottom_toTopOf="@+id/fullAmountHomePage"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/creditNameHomePage"
                app:layout_constraintTop_toBottomOf="@+id/creditNameHomePage" />

            <TextView
                android:id="@+id/DeadlineHomePage"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginEnd="165dp"
                android:layout_marginRight="165dp"
                android:text="TextView"
                app:layout_constraintBottom_toBottomOf="@+id/fullAmountHomePage"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/fullAmountHomePage"
                app:layout_constraintTop_toTopOf="@+id/fullAmountHomePage" />

            <TextView
                android:id="@+id/fullAmountHomePage"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginStart="128dp"
                android:layout_marginLeft="128dp"
                android:layout_marginEnd="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginBottom="16dp"
                android:text="TextView"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/DeadlineHomePage"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/creditornameHomePage" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>